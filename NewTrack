                            var timeString2 = Todaytable[i].ActualInTime;
                            var H2 = +timeString2.substr(11, 2);
                            var h2 = H2 % 12 || 12;
                            var ampm2 = (H2 < 12 || H2 === 24) ? "AM" : "PM";
                            timeString2 = h2 + timeString2.substr(13, 3) + ampm2;

                            var timeString3 = Todaytable[i].ActualOutTime;
