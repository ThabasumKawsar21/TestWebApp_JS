                        var timeString1 = Todaytable[i].ToTime;
                        var H1 = +timeString1.substr(0, 2);
                        var h1 = H1 % 12 || 12;
                        var ampm1 = (H1 < 12 || H1 === 24) ? "AM" : "PM";
                        timeString1 = h1 + timeString1.substr(2, 3) + ampm1;
