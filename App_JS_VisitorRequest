                SucessModel.style.display = "block";
                $("#spnerror").hide();
                $(".editimage").hide();
                $(".cancelimage").hide();
                $(".divLocationdetails").remove();
                document.getElementById("ddlpurpose").value = 0;
                $("#txtvisitdate").datepicker('update', '');
                $("#txtstarttime").val("09:00");
                $("#txtendtime").val("18:00");
                $("#txthostID").val("");
                $("#ddllocationlist").val("0");
