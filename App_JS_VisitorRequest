            var isDatacenterAccess = "";
            if (isDatacenterAccesscheck == "Update Datacenter Access Details") {
                isDatacenterAccess = "1";
            }
            else {
                isDatacenterAccess = "0";
            }
            if (LocationCount == undefined || LocationCount == "" || LocationCount == null) {
                LocationCount = 0;
