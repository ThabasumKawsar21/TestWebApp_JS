                            });
                        }


                    },
                    error: function (result) {
                        $("#fade").hide();
                        $("#modal").hide();

                    }
                });
                var CurrentLocation = document.getElementById("hdnLocationCode").value;
                $('#ddllocationlist').val(CurrentLocation);
