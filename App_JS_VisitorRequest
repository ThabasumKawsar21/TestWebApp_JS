            $("#modal").hide();

        }
    });
    document.getElementById("ddlpurpose").value = "0";
    $('#ddlpurpose').css("border", "1px solid #0033a0");
    $('#ddllocationlist').val("0");
    $('#ddllocationlist').trigger('change');
    $('#sandbox-container input').datepicker('update', '');
