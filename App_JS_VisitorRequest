                                            }
                                            Cart_append += "</td>";
                                            Cart_append += "<td class='Recentvisitortabletd' title=\"" + Cart[i].LocationName + "\">";
                                            Cart_append += "" + Cart[i].LocationName.slice(0, parseInt(15)) + (Cart[i].LocationName.length > parseInt(15) ? "..." : "") + "";
