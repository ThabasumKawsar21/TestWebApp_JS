                        if (Splitdate.length == 1) {
                            ConcaSpt = Splitdate;
                        }
                        else {
                            var ConcaSpt = "";
                            for (var w = 0; w < Splitdate.length; w++) {
                                var splitsingledata = Splitdate[w].split('/');
                                ConcaSpt += splitsingledata[0] + "/" + splitsingledata[1] + ",";
                            }
